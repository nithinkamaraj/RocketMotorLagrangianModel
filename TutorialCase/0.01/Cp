FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0.01";
    object      Cp;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 -1 0 0 0];

internalField   uniform 1069.994677;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    wall
    {
        type            calculated;
        value           uniform 1069.994677;
    }
    inlet
    {
        type            calculated;
        value           uniform 1069.994677;
    }
    outlet
    {
        type            calculated;
        value           uniform 1069.994677;
    }
}


